package Trabalho;

public class Controle {

	public static void main(String[] args) {
		Topic zona = new Topic(10, 2);
		zona.embarcar(new Pass("João", 20, "1111"), 20);
		zona.embarcar(new Pass("José", 62, "2222"), 62);
		zona.embarcar(new Pass("Maria", 74, "3333"), 74);
		zona.embarcar(new Pass("Pedro", 82, "4444"), 82);
		zona.embarcar(new Pass("P", 82, "4444"), 82);
		zona.embarcar(new Pass("Pe", 82, "4444"), 82);
		zona.embarcar(new Pass("Ped", 82, "4444"), 82);
		zona.embarcar(new Pass("Pedr", 82, "4444"), 82);
		zona.embarcar(new Pass("o", 82, "4444"), 82);
		zona.embarcar(new Pass("ro", 82, "4444"), 82);
		zona.embarcar(new Pass("dro", 82, "4444"), 82);
		System.out.println(zona);
	}

}
