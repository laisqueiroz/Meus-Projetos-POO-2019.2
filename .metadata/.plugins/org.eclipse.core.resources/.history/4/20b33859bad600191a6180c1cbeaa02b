package saladeaula;

import java.util.ArrayList;

public class Aluno {
	private String nome;
	private int idade;
	static int nextId = 0;
	
	
	ArrayList<String> alunos = new ArrayList<String>();
	
	public Aluno(){
		
	}
	public Aluno(String name, int idd){
		this.nome = name;
		this.idade = idd;
		alunos.add(this.nome);
		
	}
}	