package trabalho;

public class Zepzap {
	Repositorio<String , User> usuarios;
	
	public Zepzap(){
		usuarios = new Repositorio<String, User>();
		
	}
	
	
	public void addUser(String nome){
		usuarios.add(nome, new User(nome));
	}

	
	public String toString() {
		String saida = "";
		for(User usuario : this.usuarios.getAll())
			saida += usuario + "\n";
		return saida;
	}

}
