package trabalho;

import java.util.ArrayList;
import java.util.Map;
import java.util.TreeMap;

public class User {
	String id;
	Map<String, User> seguindo;
	Map<String, User> seguidores;
	ArrayList<String> timeline;
	
	public User(String id) {
		super();
		this.id = id;
		criarConta(id);
	}
	
	public void criarConta(String id){
		seguindo = new TreeMap<String, User>();
		seguidores = new TreeMap<String, User>();
	}
	
	
}
