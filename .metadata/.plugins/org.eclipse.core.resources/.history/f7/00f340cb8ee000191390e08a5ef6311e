package Trabalho;

import java.util.ArrayList;

class Pass{
	String id;
	String ticket;
	
	public Pass(String id, String ticket) {
		super();
		this.id = id;
		this.ticket = ticket;
	}

	public String toString() {
		return "Nome" + id + ": " + ticket;
	}
	
	
	
}
