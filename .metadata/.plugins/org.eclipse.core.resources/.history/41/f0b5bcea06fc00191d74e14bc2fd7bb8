package trabalho;

public class Superior {
	public void seguir(String quemSeguindo, User quem){
		for (String s : usuarios){
				
			if (s == null)
					throw new RuntimeException(id + "nao existe");
				
			else
				seguindo.put(quemSeguindo, quem);
				seguidores.put(quemSeguindo, quem);
			}
	}
}
