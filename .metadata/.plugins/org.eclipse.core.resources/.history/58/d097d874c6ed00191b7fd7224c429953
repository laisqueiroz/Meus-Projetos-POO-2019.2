package agencia;

public abstract class Conta {
	private double saldo;
	private int id;
	private Cliente cliente;
	
	abstract String getTipo();
	
	abstract void update();
	
	@Override
	public String toString() {
		return this.id + ":" + this.cliente.getId() + ":" + this.saldo + ":" + this.getTipo();
	}
	

}
